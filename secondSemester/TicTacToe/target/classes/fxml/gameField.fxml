<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="200" minWidth="600" prefHeight="700.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ru.spbau.egorov.hw_2.controller.MainController">
    <GridPane prefHeight="600.0" prefWidth="600.0">
        <columnConstraints>
            <ColumnConstraints hgrow="ALWAYS" prefWidth="200.0"/>
            <ColumnConstraints hgrow="ALWAYS" prefWidth="200.0"/>
            <ColumnConstraints hgrow="ALWAYS" prefWidth="200.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="50.0" prefHeight="200.0" vgrow="ALWAYS"/>
            <RowConstraints minHeight="50.0" prefHeight="200.0" vgrow="ALWAYS"/>
            <RowConstraints minHeight="50.0" prefHeight="200.0" vgrow="ALWAYS"/>
        </rowConstraints>
        <Button onAction="#onClickMethod" mnemonicParsing="false" maxHeight="Infinity"
                maxWidth="Infinity"
                GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <Button onAction="#onClickMethod" mnemonicParsing="false" maxHeight="Infinity"
                maxWidth="Infinity"
                GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <Button onAction="#onClickMethod" mnemonicParsing="false" maxHeight="Infinity"
                maxWidth="Infinity"
                GridPane.columnIndex="0" GridPane.rowIndex="2"/>
        <Button onAction="#onClickMethod" mnemonicParsing="false" maxHeight="Infinity"
                maxWidth="Infinity"
                GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <Button onAction="#onClickMethod" mnemonicParsing="false" maxHeight="Infinity"
                maxWidth="Infinity"
                GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Button onAction="#onClickMethod" mnemonicParsing="false" maxHeight="Infinity"
                maxWidth="Infinity"
                GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Button onAction="#onClickMethod" mnemonicParsing="false" maxHeight="Infinity"
                maxWidth="Infinity"
                GridPane.columnIndex="2" GridPane.rowIndex="0"/>
        <Button onAction="#onClickMethod" mnemonicParsing="false" maxHeight="Infinity"
                maxWidth="Infinity"
                GridPane.columnIndex="2" GridPane.rowIndex="1"/>
        <Button onAction="#onClickMethod" mnemonicParsing="false" maxHeight="Infinity"
                maxWidth="Infinity"
                GridPane.columnIndex="2" GridPane.rowIndex="2"/>
    </GridPane>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="600.0">
        <Button mnemonicParsing="false" onAction="#toStartMenu" prefHeight="100.0" prefWidth="200.0"
                text="Menu"/>
    </HBox>
</VBox>
