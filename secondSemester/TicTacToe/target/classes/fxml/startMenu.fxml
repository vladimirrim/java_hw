<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="800.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ru.spbau.egorov.hw_2.controller.MainController">
    <Button fx:id="hotSeat" mnemonicParsing="false" onAction="#toGameField" style="-fx-font: 30px font" text="hot seat"
            prefHeight="200.0"
            prefWidth="200.0"/>
    <Button fx:id="easy" mnemonicParsing="false" onAction="#toGameField" text="easy bot" style="-fx-font: 30px font"
            prefHeight="200.0"
            prefWidth="200.0"/>
    <Button fx:id="hard" mnemonicParsing="false" onAction="#toGameField" text="hard bot" style="-fx-font: 30px font"
            prefHeight="200.0"
            prefWidth="200.0"/>
    <Button fx:id="statistics" mnemonicParsing="false" onAction="#toGameStats" text="statistics"
            style="-fx-font: 30px font"
            prefHeight="200.0"
            prefWidth="200.0"/>
</VBox>
